{
  "expo": {
    "name": "com.testing.expo-typescript-babel7-jest",
    "version": "0.0.1",
    "slug": "expo-typescript-babel7-jest",
    "android": {
      "package": "com.testing.expo",
      "versionCode": 22,
      "permissions": []
    },
    "ios": {
      "bundleIdentifier": "com.testing.expo"
    },
    "assetBundlePatterns": [
      "./src/assets/**"
    ],
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "sdkVersion": "30.0.0",
    "packagerOpts": {
      "sourceExts": [
        "ts",
        "tsx"
      ],
      "transformer": "node_modules/react-native-typescript-transformer/index.js"
    },
    "orientation": "portrait"
  }
}
